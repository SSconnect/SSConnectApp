{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "react",
    "react-native",
    "import",
    "flowtype"
  ],
  "globals": {
    "__DEV__": true,
    "alert": true
  },
  "rules": {
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "indent": [
      "error",
      "tab"
    ],
    "react/jsx-filename-extension": 0,
    "no-tabs": 0
  }
}
